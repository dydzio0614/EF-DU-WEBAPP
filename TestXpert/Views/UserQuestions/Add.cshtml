@model TestXpert.Models.Question

@{
    ViewData["Title"] = "Create";
}

<h2>Add existing Question</h2>

<h4>Question</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Add">
            <div class="form-group">
                <label class="control-label">Select question</label>
                @Html.DropDownList("dbQuestions", "Select question")
            </div>
            <div class="form-group">
                <input type="submit" value="Add" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>
